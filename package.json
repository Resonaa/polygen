{
  "name": "polygen",
  "sideEffects": false,
  "scripts": {
    "bench": "run-s bench:*",
    "bench:hash": "ts-node benches/hash.ts",
    "build": "remix build",
    "dev": "remix dev -c \"node build/server/index.js\"",
    "format": "prettier --write . && npm run lint -- --fix",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "migrate": "npx prisma migrate dev",
    "postinstall": "patch-package",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production npx pm2 start --name polygen build/server/index.js",
    "typecheck": "tsc -b",
    "wasm": "npm run wasm:server && npm run wasm:client",
    "wasm:server": "wasm-pack build wasm --out-dir server --target nodejs --weak-refs",
    "wasm:client": "wasm-pack build wasm --out-dir client --target web --weak-refs -- --features client"
  },
  "eslintIgnore": [
    "/node_modules",
    "/public/build",
    "/server/build",
    "**/*.js"
  ],
  "prettier": {
    "trailingComma": "none",
    "arrowParens": "avoid",
    "plugins": [
      "prettier-plugin-prisma"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "latest",
    "@chakra-ui/react": "latest",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@fastify/early-hints": "latest",
    "@fastify/one-line-logger": "latest",
    "@fastify/static": "latest",
    "@fontsource-variable/fira-code": "latest",
    "@fontsource-variable/noto-sans-sc": "latest",
    "@mcansh/remix-fastify": "latest",
    "@prisma/client": "latest",
    "@remix-run/css-bundle": "latest",
    "@remix-run/node": "latest",
    "@remix-run/react": "latest",
    "bcryptjs": "latest",
    "chakra-ui-markdown-renderer": "latest",
    "clsx": "latest",
    "cross-env": "latest",
    "dayjs": "latest",
    "dotenv": "latest",
    "fastify": "latest",
    "fastify-socket.io": "latest",
    "framer-motion": "latest",
    "fs-extra": "latest",
    "i18next": "latest",
    "i18next-browser-languagedetector": "latest",
    "i18next-chained-backend": "latest",
    "i18next-fs-backend": "latest",
    "i18next-http-backend": "latest",
    "i18next-localstorage-backend": "latest",
    "isbot": "latest",
    "katex": "latest",
    "lodash": "latest",
    "lz-string": "latest",
    "nprogress": "latest",
    "openskill": "latest",
    "pixi.js": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-i18next": "latest",
    "react-icons": "latest",
    "react-markdown": "8",
    "react-textarea-autosize": "latest",
    "rehype-katex": "6",
    "remark-gfm": "3",
    "remark-math": "5",
    "remix-i18next": "latest",
    "remix-utils": "latest",
    "semantic-ui-react": "latest",
    "sharp": "latest",
    "socket.io": "latest",
    "socket.io-client": "latest",
    "socket.io-msgpack-parser": "latest",
    "source-map-support": "latest",
    "svg-captcha": "latest",
    "tiny-invariant": "latest",
    "ts-node": "latest",
    "typescript": "latest",
    "wasm-server": "file:wasm/server",
    "zod": "latest"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "latest",
    "@remix-run/dev": "latest",
    "@remix-run/eslint-config": "latest",
    "@types/bcryptjs": "latest",
    "@types/eslint": "latest",
    "@types/fs-extra": "latest",
    "@types/katex": "latest",
    "@types/lodash": "latest",
    "@types/node": "latest",
    "@types/nprogress": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/source-map-support": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-import-resolver-typescript": "latest",
    "eslint-plugin-chakra-ui": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-markdown": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "npm-run-all": "latest",
    "patch-package": "latest",
    "pm2": "latest",
    "prettier": "latest",
    "prettier-plugin-prisma": "latest",
    "prisma": "latest",
    "tinybench": "latest",
    "tsconfig-paths": "latest",
    "types-fastify-socket.io": "latest"
  },
  "engines": {
    "node": ">=20"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "optionalDependencies": {
    "bufferutil": "latest",
    "utf-8-validate": "latest"
  }
}
