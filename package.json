{
  "name": "polygen",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "bench": "vitest bench",
    "build": "remix vite:build",
    "db": "prisma generate && prisma migrate deploy && prisma db seed",
    "dev": "cross-env NODE_ENV=development tsx server.ts",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "migrate": "prisma migrate dev",
    "seed": "tsx prisma/seed.ts",
    "start": "cross-env NODE_ENV=production pm2 start --interpreter node --interpreter-args \"--import tsx\" --name polygen server.ts",
    "test": "vitest",
    "typecheck": "tsc -b"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "**/*.js"
  ],
  "prettier": {
    "trailingComma": "none",
    "arrowParens": "avoid",
    "plugins": [
      "prettier-plugin-prisma"
    ]
  },
  "dependencies": {
    "@chakra-ui/anatomy": "latest",
    "@chakra-ui/icons": "latest",
    "@chakra-ui/react": "latest",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@fastify/deepmerge": "latest",
    "@fastify/middie": "latest",
    "@fastify/one-line-logger": "latest",
    "@fastify/static": "latest",
    "@fontsource-variable/fira-code": "latest",
    "@fontsource-variable/noto-sans-sc": "latest",
    "@mcansh/remix-fastify": "latest",
    "@paralleldrive/cuid2": "latest",
    "@prisma/client": "latest",
    "@remix-run/node": "latest",
    "@remix-run/react": "latest",
    "bcryptjs": "latest",
    "chakra-ui-markdown-renderer": "latest",
    "clsx": "latest",
    "cross-env": "latest",
    "dayjs": "latest",
    "dotenv": "latest",
    "fastify": "latest",
    "fastify-socket.io": "latest",
    "framer-motion": "latest",
    "i18next": "latest",
    "i18next-browser-languagedetector": "latest",
    "isbot": "latest",
    "katex": "latest",
    "lodash": "latest",
    "lz-string": "latest",
    "nprogress": "latest",
    "openskill": "latest",
    "pixi.js": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-i18next": "latest",
    "react-icons": "latest",
    "react-markdown": "^8.0.7",
    "react-textarea-autosize": "latest",
    "rehype-katex": "^6.0.3",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remix-i18next": "latest",
    "remix-utils": "latest",
    "semantic-ui-react": "latest",
    "sharp": "latest",
    "socket.io": "latest",
    "socket.io-client": "latest",
    "socket.io-msgpack-parser": "latest",
    "source-map-support": "latest",
    "svg-captcha": "latest",
    "tiny-invariant": "latest",
    "type-fest": "latest",
    "typescript": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "latest",
    "@remix-run/dev": "latest",
    "@remix-run/eslint-config": "latest",
    "@remix-run/server-runtime": "latest",
    "@types/bcryptjs": "latest",
    "@types/eslint": "latest",
    "@types/katex": "latest",
    "@types/lodash": "latest",
    "@types/node": "latest",
    "@types/nprogress": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/source-map-support": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "latest",
    "eslint-import-resolver-typescript": "latest",
    "eslint-plugin-chakra-ui": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-markdown": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "less": "latest",
    "pm2": "latest",
    "prettier": "latest",
    "prettier-plugin-prisma": "latest",
    "prisma": "latest",
    "tsx": "latest",
    "types-fastify-socket.io": "latest",
    "vite": "latest",
    "vite-tsconfig-paths": "latest",
    "vitest": "latest"
  },
  "engines": {
    "node": ">=20"
  },
  "prisma": {
    "seed": "pnpm seed"
  },
  "optionalDependencies": {
    "bufferutil": "latest",
    "utf-8-validate": "latest"
  },
  "packageManager": "pnpm@8.15.6",
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "linux",
        "current"
      ],
      "cpu": [
        "x64",
        "arm64",
        "current"
      ]
    }
  }
}
