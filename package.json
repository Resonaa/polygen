{
  "name": "polygen",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev -c \"ts-node -r tsconfig-paths/register server/index.ts --files\"",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "migrate": "npx prisma migrate dev",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register server/index.ts --files",
    "typecheck": "tsc -b"
  },
  "eslintIgnore": [
    "/build",
    "/node_modules",
    "/public/build",
    "/server/build"
  ],
  "dependencies": {
    "@fastify/compress": "latest",
    "@fastify/one-line-logger": "latest",
    "@fastify/rate-limit": "latest",
    "@mcansh/remix-fastify": "latest",
    "@prisma/client": "latest",
    "@remix-run/node": "latest",
    "@remix-run/react": "latest",
    "bcryptjs": "latest",
    "clsx": "latest",
    "cross-env": "latest",
    "dotenv": "latest",
    "fastify": "latest",
    "fastify-https-redirect": "latest",
    "fastify-socket.io": "latest",
    "fs-extra": "latest",
    "highlight.js": "latest",
    "isbot": "latest",
    "katex": "latest",
    "lodash": "latest",
    "lz-string": "latest",
    "pixi.js": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-textarea-autosize": "latest",
    "semantic-ui-css": "latest",
    "semantic-ui-react": "latest",
    "sharp": "latest",
    "socket.io": "latest",
    "socket.io-client": "latest",
    "svg-captcha": "latest",
    "tiny-invariant": "latest",
    "ts-node": "latest",
    "typescript": "latest",
    "vditor": "latest"
  },
  "devDependencies": {
    "@remix-run/dev": "latest",
    "@remix-run/eslint-config": "latest",
    "@types/bcryptjs": "latest",
    "@types/eslint": "latest",
    "@types/fs-extra": "latest",
    "@types/katex": "latest",
    "@types/lodash": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "eslint": "latest",
    "prisma": "latest",
    "tailwindcss": "latest",
    "tsconfig-paths": "latest",
    "types-fastify-socket.io": "latest"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "optionalDependencies": {
    "bufferutil": "latest",
    "utf-8-validate": "latest"
  }
}
